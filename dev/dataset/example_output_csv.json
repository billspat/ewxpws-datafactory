{
	"name": "example_output_csv",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorageEwxDev",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileName": {
				"type": "string",
				"defaultValue": "ex_function_output"
			},
			"Extension": {
				"type": "string",
				"defaultValue": "csv"
			},
			"Timestamp": {
				"type": "string",
				"defaultValue": "2022-06-01-00-00"
			},
			"StationID": {
				"type": "string",
				"defaultValue": "STATION_ID"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat(dataset().FileName,'_',dataset().StationID,'_',dataset().Timestamp,dataset().Extension)",
					"type": "Expression"
				},
				"container": "ewx-data"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": [
			{
				"name": "station_id",
				"type": "String"
			},
			{
				"name": "request_datetime",
				"type": "String"
			},
			{
				"name": "pcpn",
				"type": "String"
			},
			{
				"name": "data_datetime",
				"type": "String"
			},
			{
				"name": "atemp",
				"type": "String"
			},
			{
				"name": "relh",
				"type": "String"
			}
		]
	}
}